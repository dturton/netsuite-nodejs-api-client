# NetSuite OAuth 1.0a Configuration
# Copy this file to .env and fill in your actual values

# Your NetSuite Account ID (e.g., 1234567)
ACCOUNT_ID=

# OAuth Consumer Key from NetSuite Integration Record
CONSUMER_KEY=

# OAuth Consumer Secret from NetSuite Integration Record
CONSUMER_SECRET=

# OAuth Token ID from NetSuite Access Token
TOKEN_ID=

# OAuth Token Secret from NetSuite Access Token
TOKEN_SECRET=